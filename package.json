{
  "name": "enflujo-historia-internet",
  "private": true,
  "packageManager": "yarn@4.9.1",
  "workspaces": [
    "aplicaciones/*"
  ],
  "scripts": {
    "dev": "yarn workspace www run dev",
    "servidor": "yarn workspace servidor run prod",
    "servidor:dev": "yarn workspace servidor run dev",
    "servidor:logs": "yarn workspace servidor run dev:logs",
    "servidor:apagar": "yarn workspace servidor run apagar",
    "build": "yarn workspace www run build",
    "lint": "prettier --check .",
    "lint:fix": "prettier --write --list-different .",
    "preview": "yarn workspace www run preview",
    "revisar": "yarn workspace www run revisar"
  },
  "devDependencies": {
    "prettier": "^3.5.3"
  }
}
